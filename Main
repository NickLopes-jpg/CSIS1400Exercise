import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	   Scanner input = new Scanner(System.in);
	   
	   System.out.println("Enter your name, favorite number, and age:");
	   
	    String name = input.nextLine();
	    int favNum = input.nextInt();
	    int age = input.nextInt();
	 
	   System.out.print(name+" your favorite number is ");
	   System.out.print(favNum+" and you are ");
	   System.out.print(age+" years old");
	}
}
